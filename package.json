{
  "name": "reasonml-postgres",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "Server.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "reasonml-postgres/library",
        "rely.lib"
      ],
      "bin": {
        "TestReasonmlPostgres.exe": "TestReasonmlPostgres.re"
      }
    },
    "library": {
      "require": [
        "digestif.c",
        "cohttp-lwt-unix",
        "graphql-lwt",
        "graphql-cohttp",
        "yojson",
        "logs.fmt",
        "fmt.tty",
        "decoders-yojson",
        "caqti",
        "caqti-lwt",
        "caqti-driver-postgresql"
      ],
      "preprocess": [
        "pps",
        "lwt_ppx"
      ]
    },
    "executable": {
      "require": [
        "reasonml-postgres/library"
      ],
      "bin": {
        "Server.exe": "Server.re"
      }
    }
  },
  "scripts": {
    "test": "esy b dune runtest",
    "start": "esy x Server.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/caqti": "*",
    "@opam/caqti-driver-postgresql": "*",
    "@opam/caqti-lwt": "*",
    "@opam/cohttp-lwt-unix": "*",
    "@opam/conf-openssl": "*",
    "@opam/decoders-yojson": "*",
    "@opam/dune": "*",
    "@opam/fmt": "*",
    "@opam/graphql": "*",
    "@opam/graphql-cohttp": "*",
    "@opam/graphql-lwt": "*",
    "@opam/jwt": "*",
    "@opam/logs": "*",
    "@opam/lwt_ppx": "*",
    "@opam/lwt_ssl": "*",
    "@opam/nocrypto": "*",
    "@opam/ppx_deriving_yojson": "*",
    "@reason-native/rely": "*",
    "ocaml": "~4.7.1000",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "esy/pesy#75d62cf",
    "@opam/conf-openssl": {
      "source": "no-source:",
      "override": {
        "dependencies": {
          "@opam/conf-pkg-config": "*",
          "@esy-packages/esy-openssl": "esy-packages/esy-openssl#65efbc9"
        }
      }
    },
    "@opam/graphql": "sync/ocaml-graphql-server:graphql.opam#638be28",
    "@opam/graphql-cohttp": "sync/ocaml-graphql-server:graphql-cohttp.opam#638be28",
    "@opam/graphql-lwt": "sync/ocaml-graphql-server:graphql-lwt.opam#638be28",
    "@opam/nocrypto": "mirleft/ocaml-nocrypto:opam#ed7bb8d911dc340e36d85d335d9edb8339f0932d",
    "@opam/jwt": "besport/ocaml-jwt:jwt.opam#6563a10"
  }
}

{
  "name": "reasonml-postgres",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "Server.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "reasonml-postgres/library",
        "rely.lib"
      ],
      "bin": {
        "TestReasonmlPostgres.exe": "TestReasonmlPostgres.re"
      }
    },
    "library": {
      "require": [
        "digestif.c",
        "cohttp-lwt-unix",
        "graphql-lwt",
        "graphql-cohttp",
        "yojson",
        "logs.fmt",
        "fmt.tty",
        "decoders-yojson"
      ]
    },
    "executable": {
      "require": [
        "reasonml-postgres/library"
      ],
      "bin": {
        "Server.exe": "Server.re"
      }
    }
  },
  "scripts": {
    "test": "esy b dune runtest",
    "start": "esy x Server.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/cohttp-lwt-unix": "*",
    "@opam/conf-openssl": "*",
    "@opam/decoders-yojson": "*",
    "@opam/dune": "*",
    "@opam/fmt": "*",
    "@opam/graphql": "*",
    "@opam/graphql-cohttp": "*",
    "@opam/graphql-lwt": "*",
    "@opam/logs": "*",
    "@opam/lwt_ssl": "*",
    "@opam/ppx_deriving_yojson": "*",
    "@reason-native/rely": "*",
    "ocaml": "~4.6.0",
    "pesy": "*",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "esy/pesy#ba6359f25621280a8105d2ffc99d75d849c0d95a",
    "@opam/conf-openssl": {
      "source": "no-source:",
      "override": {
        "dependencies": {
          "@opam/conf-pkg-config": "*",
          "@esy-packages/esy-openssl": "esy-packages/esy-openssl#65efbc9"
        }
      }
    }
  }
}
